{
    "metadata": [
        {
            "src": [
                {
                    "files": [ "**/*.csproj" ],
                    "exclude": [ "**/bin/**", "**/obj/**" ]
                }
            ],
            "dest": "obj/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [ "**/*.yml" ],
                "cwd": "obj/"
            },
            {
                "files": [ "**.md", "toc.yml" ]
            }
        ],
        "resource": [
            {
                "files": [ "**/images/**" ]
            }
        ],
        "externalReference": [
        ],
        "globalMetadata": {
            "_appTitle":  "Code Endeavors Distributed Cache"
        },
        "template": [
            "default"
        ],
        "dest": "_site"
    }
}